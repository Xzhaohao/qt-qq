QTreeView {
    border-style: none;
}

QTreeView::item {
    color: rgba(255, 255, 255, 0);
}

QTreeView::item:selected:active {
    background-color: rgba(135, 206, 250, 153);
}

QTreeView::item:selected: !active, QTreeView::item:hover {
    background-color: rgba(135, 206, 250, 50);
}

QLabel#userName {
    font-size: 14px;
    color: #000000;
}

QLabel#signName {
    font-size: 12px;
    color: #808080;
}